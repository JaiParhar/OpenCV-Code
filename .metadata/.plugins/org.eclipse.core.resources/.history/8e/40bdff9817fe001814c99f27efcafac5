import org.opencv.videoio.VideoCapture;

public class VideoSource {

	VideoCapture vSource;
	
	public VideoSource(int source) {
		vSource = new VideoCapture(source);
	}
	
	public VideoSource(String source) {
		vSource = new VideoCapture(source);
	}
	
	private void init() {
		
	}
	
	public void getFrame() {
		
	}
	
}
